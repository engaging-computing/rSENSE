<h1>Listing experiment_sessions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>User</th>
    <th>Experiment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @experiment_sessions.each do |experiment_session| %>
  <tr>
    <td><%= experiment_session.title %></td>
    <td><%= experiment_session.content %></td>
    <td><%= experiment_session.user_id %></td>
    <td><%= experiment_session.experiment_id %></td>
    <td><%= link_to 'Show', experiment_session %></td>
    <td><%= link_to 'Edit', edit_experiment_session_path(experiment_session) %></td>
    <td><%= link_to 'Destroy', experiment_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Experiment session', new_experiment_session_path %>
