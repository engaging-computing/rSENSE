<% if @cur_user.admin or @cur_user = @data_set.owner %>
  <h1>Editing Data</h1>

  <table id="editTable">
    <thead>
      <tr>
        <% @project.fields.each do |f| %>
          <th><%= f.name %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @mongo_data_set.data.each_with_index do |row, r_index| %>
        <tr>
          <% row.each_with_index do |data, d_index| %>
            <td><input type="text" class="input" value="<%= data["#{@project.fields[d_index].id}"] %>" /></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>

  <% link_to 'Show', @data_set %>
<%else%>
  You are not authorized to view this page.
<%end%>