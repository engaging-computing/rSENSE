<% if @pid.nil? %>
  <div class="span12">
    <h1 class="center">Want to import an experiment from oldSENSE?</h1>
  </div>
  <div class="span12">
    <div class="row">
      <div class="span4 offset2">
        <h2 class="pull-right">Experiment id:</h2>
      </div>
      <div class="span4" style="height:100%">
        <form style="padding-top: 20px;" accept-charset="UTF-8" action="/projects/import" method="get" class="vcenter">
          <input type="text" id="pid" name="pid" />
          <input class="btn" type="submit" value="Import" />
        </form>
      </div>
      
  </div>
<%end%>