<h1>iSENSE Users</h1>

<table class="table"> 
  <tr>
    <th>Name</th>
    <th>Username</th>
  </tr>

<br>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to (user.name), user_path(user) %></td>
    <td><%= user.username %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
