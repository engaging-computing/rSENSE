<% @contributions.each do |contribution| %>
  <div class="<%= cycle('feed-even', 'feed-odd')%>">
	<% if contribution.class.to_s == "Experiment" %>
	  <div>
	    <h4><%= link_to contribution.title, experiment_path(contribution) %><%if contribution.featured%><span style="color:#57C142"> (featured)</span><%end%></h4>
        <b>Created: </b><%= time_ago_in_words(contribution.created_at)%> ago (on <%=contribution.created_at.strftime("%B %d, %Y")%>)
      </div>
    <% elsif contribution.class.to_s == "ExperimentSession" %>
	  <div>
	    <h4><%= link_to contribution.title, "/experiments/#{contribution.experiment_id}/sessions/#{contribution.id}" %></h4> 
        <b>Created: </b><%= time_ago_in_words(contribution.created_at)%> ago (on <%=contribution.created_at.strftime("%B %d, %Y")%>)
      </div>
    <% end%>
  </div>
<% end%> 